<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext traffic to 127.0.0.1, which we use for development. -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">127.0.0.1</domain>
        <domain includeSubdomains="true">192.168.1.2</domain>
        <domain includeSubdomains="true">codeka.com.au</domain>
        <domain includeSubdomains="true">war-worlds.com</domain>
    </domain-config>
</network-security-config>
